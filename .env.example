# Core Settings
ADMIN_KEY=your-admin-key-here
LISTEN=:3000

# Database Configuration
SQL_DSN=postgres://gateway:password@localhost:5432/gateway
REDIS=redis://localhost:6379

# Cost Optimization
ENABLE_COST_OPTIMIZATION=true
COST_OPTIMIZATION_AGGRESSIVE=true
MAX_COST_PER_REQUEST=1.00
FALLBACK_TO_PAID_ON_FAILURE=true

# Unofficial APIs
ENABLE_UNOFFICIAL_APIS=true
UNOFFICIAL_BING_COOKIE=your-_U-cookie-here
BING_COOKIE_ROTATION_ENABLED=true
UNOFFICIAL_API_TIMEOUT=30s

# Provider Configuration
PROVIDERS_CSV=/app/providers.csv
POLLINATIONS_ENABLED=true

# Session Management
SESSION_POOL_SIZE=10
SESSION_ROTATION_INTERVAL=1h
HEALTH_CHECK_INTERVAL=5m

# Task Master
PARALLEL_EXECUTION_ENABLED=true
MAX_PARALLEL_TASKS=5
TASK_TIMEOUT=60s
RESULT_AGGREGATION_TIMEOUT=120s

# Monitoring
CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
CIRCUIT_BREAKER_TIMEOUT=10m
PROVIDER_RETRY_ATTEMPTS=3

# API Keys (add your actual keys)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
AZURE_OPENAI_API_KEY=
GOOGLE_AI_API_KEY=